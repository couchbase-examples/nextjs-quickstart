{
  "name": "nextjs-quickstart",
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "dev:default": "env-cmd -f .env.default next dev",
    "build": "npm explore couchbase -- npm run help-prune && npm list --depth=1 | grep couchbase && ls -a ./node_modules/@couchbase && next build && npm run load-sample-data:no-env && npm run build-indexes:no-env",
    "start": "next start",
    "lint": "next lint",
    "format:check": "prettier --check ./**/*.js",
    "format:write": "prettier --write ./**/*.js",
    "check": "npm run format:check && npm run lint",
    "init-db:default": "env-cmd -f .env.default node ./util/initializeCbServer.js",
    "init-db:local": "env-cmd -f .env.local node ./util/initializeCbServer.js",
    "build-indexes": "env-cmd -f .env.local node ./util/buildIndexes.js",
    "build-indexes:no-env": "node ./util/buildIndexes.js",
    "load-sample-data": "env-cmd -f .env.local node ./util/loadSampleData.js",
    "load-sample-data:no-env": "node ./util/loadSampleData.js",
    "test": "env-cmd -f .env.default jest"
  },
  "dependencies": {
    "@headlessui/react": "^2.1.1",
    "@heroicons/react": "^2.1.4",
    "axios": "^1.7.2",
    "clsx": "^2.1.1",
    "couchbase": "^4.4.0",
    "next": "^14.2.4",
    "qs": "^6.12.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "env-cmd": "^10.1.0",
    "eslint": "^8.45.0",
    "eslint-config-next": "^14.2.4",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "next-test-api-route-handler": "^4.0.8",
    "postcss": "^8.4.39",
    "prettier": "^3.3.2",
    "tailwindcss": "^3.4.4"
  },
  "license": "MIT"
}
