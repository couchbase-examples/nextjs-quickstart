{
  "name": "nextjs-quickstart",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "dev:default": "env-cmd -f .env.default next dev",
    "build": "npm run strip-cb-sdk && next build",
    "start": "next start",
    "lint": "next lint",
    "format:check": "prettier --check ./**/*.js",
    "format:write": "prettier --write ./**/*.js",
    "check": "npm run format:check && npm run lint",
    "check:ci": "npm run check && npm run test",
    "init-db:default": "env-cmd -f .env.default npx babel-node ./util/initializeCbServer.js",
    "init-db:local": "env-cmd -f .env.local npx babel-node ./util/initializeCbServer.js",
    "build-indexes": "env-cmd -f .env.local npx babel-node ./util/buildIndexes.js",
    "strip-cb-sdk": "strip --strip-debug ./node_modules/couchbase/build/Release/couchbase_impl.node",
    "test": "env-cmd -f .env.default jest",
    "test:f": "env-cmd -f .env.default jest __test__/api-profiles-create.test.js"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.15",
    "@heroicons/react": "^2.0.18",
    "axios": "^1.3.4",
    "bcryptjs": "^2.4.3",
    "clsx": "^1.2.1",
    "couchbase": "^4.2.2",
    "next": "13.2.4",
    "node-fetch": "^3.3.1",
    "qs": "^6.11.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "set-value": ">=4.1.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.3",
    "@babel/node": "^7.20.7",
    "@babel/plugin-transform-runtime": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "autoprefixer": "^10.4.14",
    "env-cmd": "^10.1.0",
    "eslint": "^8.44.0",
    "eslint-config-next": "^13.4.9",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^29.5.0",
    "postcss": "^8.4.24",
    "prettier": "^3.0.0",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.3.2",
    "test-listen": "^1.1.0"
  },
  "license": "MIT"
}
